  "related_applications": [],
  "prefer_related_applications": false,
  
  "protocol_handlers": [
    {
      "protocol": "web+strcertified",
      "url": "/handle?protocol=%s"
    }
  ],
  
  "file_handlers": [
    {
      "action": "/handle-inspection",
      "accept": {
        "application/vnd.str-inspection": [".strinsp"],
        "application/json": [".json"]
      },
      "launch_type": "single-client"
    }
  ],
  
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "video/*", ".pdf", ".doc", ".docx"]
        }
      ]
    }
  },
  
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "handle_links": "preferred",
  
  "id": "str-certified-pwa",
  
  "note_taking": {
    "new_note_url": "/inspection/new-note"
  },
  
  "scope_extensions": [
    {
      "origin": "https://str-certified.com"
    },
    {
      "origin": "https://api.str-certified.com"
    }
  ],
  
  "display_override": [
    "window-controls-overlay",
    "minimal-ui",
    "standalone",
    "browser"
  ],
  
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7"
